{
  "manifest_version": 3,
  "name": "Jupiter Extension",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://example.com/*",
    "http://localhost/*",
    "file:///*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
